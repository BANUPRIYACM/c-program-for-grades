#include<stdio.h>
void main()
{
int num;
printf("enter the marks");
scanf("%d"&num);
printf("you entered %d",num);
if(num>=100)
printf("you got A grade");
else if (num>=80)
printf("you got B grade");
else if (num>=60)
printf("you got B grade");
else if (num>=40)
printf("you got B grade");
else if (num<40)
printf("you got F grade");
}
return 0;
}

